# game/scripts/A_home/Inside/Living/M/scenes/LiR1_MAS1a.rpy:54
translate french LiR1_MAS1a_label_6f1cfb56:

    # Yazmin "You’re exaggerating, Liza. The only thing - I - really do, is model clothes, for local chains."
    Yazmin "Tu exagères, Liza. La seule chose que - je - fais vraiment, ce sont des vêtements de mannequin, pour les chaînes locales."

# game/scripts/A_home/Inside/Living/M/scenes/LiR1_MAS1a.rpy:55
